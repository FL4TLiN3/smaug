<h2>Show category</h2>

<ul>

  <li>
    <strong>Name:</strong>
    <%= @category.name %>
  </li>

  <li>
    <strong>Order:</strong>
    <%= @category.order %>
  </li>

  <li>
    <strong>Published:</strong>
    <%= @category.published %>
  </li>

</ul>

<%= link "Edit", to: category_path(@conn, :edit, @category) %>
<%= link "Back", to: category_path(@conn, :index) %>
